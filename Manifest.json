{
  "name": "Phishing Alert",
  "version": "1.0",
  "manifest_version": 3,
  "description": "A Chrome extension to protect users from phishing attacks.",
  "permissions": [
    "activeTab",
    "storage",
    "tabs"
  ],

  "action": {
    "default_popup": "index.html",
    "default_icon": {
      "16": "images/icon16.png",
      "48": "images/icon48.png",
      "128":"images/icon128.png"
    }
  }
}
